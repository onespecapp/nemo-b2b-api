{
  "name": "nemo-b2b-api",
  "version": "1.0.0",
  "description": "Nemo B2B - Automated appointment reminder calls API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc -w & nodemon dist/index.js",
    "dev:once": "tsc && node dist/index.js",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "seed": "ts-node scripts/seed-demo-data.ts",
    "seed:clear": "ts-node scripts/seed-demo-data.ts --clear",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "appointments",
    "reminders",
    "calls",
    "b2b"
  ],
  "author": "Nemo Cares",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^1.39.0",
    "@prisma/client": "^5.22.0",
    "@supabase/supabase-js": "^2.49.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "livekit-server-sdk": "^2.15.0",
    "telnyx": "^5.17.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.10",
    "@types/ws": "^8.18.1",
    "@types/yauzl": "^2.10.3",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  }
}
